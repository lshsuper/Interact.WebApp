
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>SiginSucc</title>
    <script src="~/Content/lib/jquery/jquery-3.3.1.min.js"></script>
    <link href="~/Content/lib/mui-master/css/mui.min.css" rel="stylesheet" />
    <script src="~/Content/lib/mui-master/js/mui.min.js"></script>
    <style>
        .sigin-succ-header > div:first-child {
            height: 70px;
            width: 70px;
            /* margin: 20px auto; */
            margin-left: 64px;
            margin-top: 25px;
            background-image: url(/Content/webappp/img/icon-sigin-succ.png);
            background-size: cover;
        }
        .sigin-succ-header > div:last-child {
            margin-right: 48px;
            margin-top: 22px;
            color: #FF5300;
        }
        .sigin-succ-header {
            padding-bottom: 27px;
        }
        .sigin-succ-body {
            padding: 15px 0px 15px 20px;
        }
      
    </style>
</head>
<body>
    <div class="mui-card">
        <!--页眉，放置标题-->
        <div class="mui-card-header sigin-succ-header">
            <div></div>
            <div>恭喜您，签到成功！</div>
        </div>
        <!--内容区-->
        <div class="mui-card-content sigin-succ-body">
            <div class="user-nickname"><span>昵称:</span><span>lsh</span></div>
            <div class="user-age"><span>年龄:</span><span>25</span></div>
            <div class="user-area"><span>地址:</span><span>北京-通州</span></div>
        </div>
       
    </div>
</body>
</html>
